<script>

    $(function () {
        Highcharts.chart('container', {
            chart: {
                type: 'column'
            },
            title: {
                text: 'Top Investment domains for 2005-15'
            },
            subtitle: {
                text: ''
            },
            xAxis: {
                categories: [
                'Mobile',
                'Cloud',
                'Big Data Analytics',
                'Healthcare / Biotechnology',
                'Software',
                'Security',
                'Enterprise',
                'Automotive',
                'Advertising',
                'Finance',
                'E-commerce',
                'Food/Hospitality'
                ],
                crosshair: true
            },
            yAxis: {
                min: 0,
                title: {
                    text: 'Number of Investments'
                }
            },
            tooltip: {
                headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                '<td style="padding:0"><b>{point.y:.1f} </b></td></tr>',
                footerFormat: '</table>',
                shared: true,
                useHTML: true
            },
            plotOptions: {
                column: {
                    pointPadding: 0.2,
                    borderWidth: 0
                }
            },
            series: [{
                name: 'Angel',
                data: [ 978, 112, 317, 215, 897, 76, 294, 60, 230, 225, 362, 20]

            }, {
                name: 'Seed',
                data: [5891, 643, 2815, 1866, 6307, 637, 2325, 222, 1826, 1674, 2489, 725]

            },  {
                name: 'Venture',
                data: [8827, 1761, 5003, 6140, 18268, 2805, 6763, 475, 4354, 2550, 3568, 795]

            }]
        });
    });     

    angular.module('myModule', [])
    .controller('myController', function($scope,$http) {
        $http.get("/getInv")
        .then(function(response) {
            Highcharts.chart('container2', {
                title: {
                    text: 'Top Investor Rounds Yearly'
                },
                xAxis: {
                    categories: response.data.inv
                },
                labels: {
                    items: [{
                        html: 'Total Funding Rounds',
                        style: {
                            left: '50px',
                            top: '18px',
                            color: (Highcharts.theme && Highcharts.theme.textColor) || 'black'
                        }
                    }]
                },
                series: [{
                    type: 'column',
                    name: '2015',
                    data: response.data.y2015
                }, {
                    type: 'column',
                    name: '2014',
                    data: response.data.y2014
                }, {
                    type: 'column',
                    name: '2013',
                    data: response.data.y2013
                }, {
                    type: 'column',
                    name: '2012',
                    data: response.data.y2012
                }, {
                    type: 'column',
                    name: '2011',
                    data: response.data.y2011
                }, {
                    type: 'spline',
                    name: 'Average',
                    data: response.data.allYear,
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'white'
                    }
                }, {
                    type: 'pie',
                    name: 'Total Rounds',
                    data: [{
                        name: '2015',
                        y: 1157,
                color: Highcharts.getOptions().colors[0] // Jane's color
            }, {
                name: '2014',
                y: 1416,
                color: Highcharts.getOptions().colors[1] // John's color
            }, {
                name: '2013',
                y: 1217,
                color: Highcharts.getOptions().colors[2] // John's color
            }, {
                name: '2012',
                y: 1137,
                color: Highcharts.getOptions().colors[3] // John's color
            }, {
                name: '2011',
                y: 988,
                color: Highcharts.getOptions().colors[4] // Joe's color
            }],
            center: [100, 80],
            size: 100,
            showInLegend: false,
            dataLabels: {
                enabled: false
            }
        }]
    });
        });

    });
</script>
