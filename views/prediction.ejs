<script type="text/javascript">

    $(function(){
        var $select = $(".dollar");
        for (i=0;i<=30000000000;i+=5000000000){
            $select.append($('<option></option>').val(i).html(i))
        }
    });

    $(document).ready(function() {
      $('#dollar').on('change', function() {
         checkChange();
     });
  });

    $(document).ready(function() {
      $('#domain').on('change', function() {
         checkChange();
     });
  });

    $(document).ready(function() {
      $('#state').on('change', function() {
         checkChange();
     });
  });


    function checkChange(){
        $.post("/reqTable",
        {
            reg:document.getElementById ("state"),
            dol:document.getElementById ("dollar"),
            dom:document.getElementById ("domain")
        },
        function(data){
            alert("Data: " + data);
        });
    }



</script>
